#include <iostream>
using namespace std;
//Program function dengan parameter dan nilai kembalian muhammad hilal zada

void cetakInfo ()
{
	cout << "Ini adalah function dengan parameter dan nilai kembalian" << endl;
}

int Pengurangan(int nilai1 , int nilai2)
{
	return  nilai1 - nilai2;
}

int main ()
{
	cetakInfo ();
	cout << "Berikut adalah hasil pengurangan dari (100,57) : " <<endl << Pengurangan(100,57);
	return 0;
}
